{
  "name": "bloodyowl",
  "version": "0.1.0",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "gh-pages -d dist",
    "server": "pages start src/App.bs.js",
    "prerender": "pages build src/App.bs.js",
    "release": "rm -fr dist && yarn build && yarn prerender && yarn deploy",
    "format": "find ./src -iname '*.re' | xargs bsrefmt --in-place && find ./src -iname '*.rei' | xargs bsrefmt -i true --in-place"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bs-css-emotion": "^2.1.0",
    "bs-webapi": "^0.13.1",
    "front-matter": "^3.0.1",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.3",
    "highlight.js": "^9.13.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "reason-future": "^2.3.1",
    "reason-highlightjs": "^0.2.0",
    "reason-react": "^0.9.1",
    "reason-react-update": "^1.0.0",
    "remarkable": "^1.7.1",
    "rescript-pages": "0.1.17"
  },
  "devDependencies": {
    "bs-platform": "^8.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "emotion-server": "^10.0.0",
    "html-webpack-plugin": "^3.2.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "webpack": "^4.0.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8"
  }
}
